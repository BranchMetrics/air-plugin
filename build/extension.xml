<extension xmlns="http://ns.adobe.com/air/extension/24.0">
    <id>com.tune.Tune</id>
    <versionNumber>5.1.0</versionNumber>

    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libTuneANE.jar</nativeLibrary>
                <initializer>com.tune.ane.TuneExtension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>libTuneANE.jar</nativeLibrary>
                <initializer>com.tune.ane.TuneExtension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTuneANE.a</nativeLibrary>
                <initializer>TuneExtInitializer</initializer>
                <finalizer>TuneExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libTuneANE.a</nativeLibrary>
                <initializer>TuneExtInitializer</initializer>
                <finalizer>TuneExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
